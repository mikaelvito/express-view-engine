<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>USERS</title>
  </head>
  <body>
    <a href="/register">Tambah User</a>
    <table border="1">
      <thead>
        <tr>
          <th>Email</th>
          <th>Password</th>
        </tr>
      </thead>

      <tbody>
        <% users.forEach((user) => { %>
        <tr>
          <td><%= user.email %></td>
          <td><%= user.password %></td>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>
